{
  "name": "antd-more",
  "version": "3.3.1",
  "description": "基于 antd 扩展的业务场景组件。",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": [
    "*.less"
  ],
  "scripts": {
    "start": "dumi dev",
    "doc": "dumi build",
    "doc-version": "cross-env BUIDL_DOC_VERSION=version npm run doc",
    "build": "father-build",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:fix": "npm run lint-fix:js && npm run lint-fix:style",
    "lint:js": "eslint src --ext .ts,.tsx",
    "lint-fix:js": "npm run lint:js -- --fix",
    "lint:style": "stylelint src/**/*.less",
    "lint-fix:style": "npm run lint:style -- --fix",
    "prettier": "prettier --write **/*",
    "lint-staged": "lint-staged",
    "precommit": "lint-staged",
    "prepublishOnly": "npm run build",
    "commit": "cz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/doly-dev/antd-more.git"
  },
  "keywords": [
    "react",
    "react-components",
    "ant-design",
    "antd",
    "antd-more"
  ],
  "files": [
    "es",
    "lib"
  ],
  "author": "caijf",
  "license": "MIT",
  "peerDependencies": {
    "@ant-design/icons": "^4.0.0",
    "antd": "^4.17.4",
    "react": ">= 16.9.0",
    "react-dom": ">= 16.9.0"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-layout": "^6.38.9",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@types/estree": "^0.0.51",
    "@types/mockjs": "^1.0.6",
    "@types/react": "^18",
    "@types/react-color": "^3.0.6",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "antd": "^4.21.3",
    "babel-plugin-import": "^1.13.5",
    "commitlint": "^17.0.2",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dumi": "^1.1.42",
    "dumi-theme-default": "^1.1.21",
    "eslint": "^8.18.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "father-build": "^1.22.3",
    "husky": "^4.3.0",
    "invariant": "^2.2.4",
    "lcn": "^4.1.0",
    "lint-staged": "^13.0.2",
    "mockjs": "^1.1.0",
    "prettier": "^2.7.1",
    "prettier-plugin-two-style-order": "^1.0.1",
    "qs": "^6.10.5",
    "react": "^18",
    "react-dom": "^18",
    "stylelint": "^14.9.1",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.5.0",
    "stylelint-no-unsupported-browser-features": "^5.0.3",
    "typescript": "4"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.3",
    "classnames": "^2.3.1",
    "countdown-pro": "^1.0.2",
    "moment": "^2.29.3",
    "rc-hooks": "^2.0.6",
    "rc-select": "^14.1.7",
    "rc-util": "^5.21.5",
    "react-color": "^2.19.3",
    "util-helpers": "^4.10.0"
  },
  "lint-staged": {
    "**/*.less": "stylelint --fix",
    "**/*.{ts,tsx,js,jsx}": "eslint",
    "**/*.{ts,tsx,js,jsx,less,md}": "prettier -w"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "resolutions": {
    "@types/react": "^18",
    "@types/react-dom": "^18"
  }
}
