{
  "name": "antd-more",
  "version": "3.8.0",
  "description": "基于 antd 扩展的业务场景组件。",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": [
    "*.less"
  ],
  "scripts": {
    "start": "dumi dev",
    "doc": "dumi build",
    "doc-version": "cross-env BUIDL_DOC_VERSION=version npm run doc",
    "build": "father build",
    "lint": "pnpm run lint:js && pnpm run lint:style",
    "lint:fix": "pnpm run lint-fix:js && pnpm run lint-fix:style",
    "lint:js": "eslint src --ext .ts,.tsx",
    "lint-fix:js": "pnpm run lint:js -- --fix",
    "lint:style": "stylelint src/**/*.less",
    "lint-fix:style": "pnpm run lint:style -- --fix",
    "prettier": "prettier --write **/*",
    "prepublishOnly": "pnpm run build",
    "commit": "cz",
    "tsc": "tsc --noEmit",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/doly-dev/antd-more.git"
  },
  "keywords": [
    "react",
    "react-components",
    "ant-design",
    "antd",
    "antd-more"
  ],
  "files": [
    "es",
    "lib"
  ],
  "author": "caijf",
  "license": "MIT",
  "peerDependencies": {
    "@ant-design/icons": ">= 4.0.0",
    "antd": "^4.24.0",
    "react": ">= 16.9.0",
    "react-dom": ">= 16.9.0"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.8.1",
    "@ant-design/pro-layout": "^6.38.22",
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@types/estree": "^0.0.51",
    "@types/mockjs": "^1.0.10",
    "@types/react": "^18.2.38",
    "@types/react-color": "^3.0.10",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "antd": "^4.24.15",
    "babel-plugin-import": "^1.13.8",
    "commitlint": "^17.8.1",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dumi": "^1.1.54",
    "dumi-theme-default": "^1.1.24",
    "element-remove": "^1.0.4",
    "eslint": "^8.54.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "father": "^4.3.7",
    "file-loader": "^6.2.0",
    "husky": "^8.0.3",
    "invariant": "^2.2.4",
    "lcn": "^5.0.6",
    "lint-staged": "^13.3.0",
    "mockjs": "^1.1.0",
    "prettier": "^2.8.8",
    "prettier-plugin-two-style-order": "^1.0.1",
    "qs": "^6.11.2",
    "rc-slider-captcha": "^1.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stylelint": "^14.16.1",
    "stylelint-config-css-modules": "^4.3.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.7.0",
    "stylelint-no-unsupported-browser-features": "^6.1.0",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.23.4",
    "cache2": "^2.0.1",
    "classnames": "^2.3.2",
    "countdown-pro": "^2.0.1",
    "moment": "^2.29.4",
    "rc-hooks": "^3.0.7",
    "rc-select": "^14.10.0",
    "rc-util": "^5.38.1",
    "react-color": "^2.19.3",
    "ut2": "^1.5.6",
    "util-helpers": "^4.20.8"
  },
  "lint-staged": {
    "**/*.less": "stylelint --fix",
    "**/*.{ts,tsx,js,jsx}": "eslint",
    "**/*.{ts,tsx,js,jsx,less,md}": "prettier -w"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "resolutions": {
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "file-system-cache": "^2.4.4"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "tag": "latest-3"
  }
}
