{
  "name": "antd-more",
  "version": "2.6.2",
  "description": "基于 antd 扩展的业务场景组件。",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": [
    "*.less"
  ],
  "scripts": {
    "start": "dumi dev",
    "doc": "dumi build",
    "doc-version": "cross-env BUIDL_DOC_VERSION=version npm run doc",
    "build": "father-build",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:fix": "npm run lint-fix:js && npm run lint-fix:style",
    "lint:js": "eslint src --ext .ts,.tsx",
    "lint-fix:js": "npm run lint:js -- --fix",
    "lint:style": "stylelint src/**/*.less",
    "lint-fix:style": "npm run lint:style -- --fix",
    "prettier": "prettier --write **/*",
    "lint-staged": "lint-staged",
    "precommit": "lint-staged",
    "commit": "cz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/doly-dev/antd-more.git"
  },
  "keywords": [
    "react",
    "react-components",
    "ant-design",
    "antd",
    "antd-more"
  ],
  "files": [
    "es",
    "lib"
  ],
  "author": "caijf",
  "license": "MIT",
  "peerDependencies": {
    "@ant-design/icons": "^4.0.0",
    "antd": "^4.0.0",
    "react": ">= 16.9.0",
    "react-dom": ">= 16.9.0"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.3.0",
    "@ant-design/pro-layout": "^6.14.0",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/classnames": "^2.2.11",
    "@types/estree": "^0.0.50",
    "@types/mockjs": "^1.0.4",
    "@types/react": "^17.0.3",
    "@types/react-color": "^3.0.4",
    "@types/react-dom": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "antd": "^4.15.1",
    "babel-plugin-import": "^1.13.3",
    "commitlint": "^11.0.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dumi": "^1.1.4",
    "dumi-theme-default": "^1.1.0",
    "eslint": "^7.28.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "father-build": "^1.18.5",
    "husky": "^4.3.0",
    "invariant": "^2.2.4",
    "lcn": "3",
    "lint-staged": "^10.5.1",
    "mockjs": "^1.1.0",
    "prettier": "^2.2.1",
    "qs": "^6.10.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "stylelint": "^14.1.0",
    "stylelint-config-css-modules": "^2.3.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.5.0",
    "stylelint-no-unsupported-browser-features": "^5.0.2",
    "typescript": "4"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.0",
    "classnames": "^2.2.6",
    "countdown-pro": "^1.0.2",
    "moment": "^2.29.1",
    "rc-hooks": "^1.8.1",
    "rc-select": "^13.2.1",
    "rc-util": "^5.16.0",
    "react-color": "^2.19.3",
    "util-helpers": "^4.0.1"
  },
  "lint-staged": {
    "**/*.less": "stylelint --fix",
    "**/*.{ts,tsx,js,jsx}": "eslint",
    "**/*.{ts,tsx,js,jsx,less,md}": "prettier -w"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
