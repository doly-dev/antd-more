{
  "name": "antd-more",
  "version": "4.1.52",
  "description": "基于 antd 扩展的业务场景组件。",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "sideEffects": [
    "*.less"
  ],
  "scripts": {
    "start": "dumi dev",
    "docs": "dumi build",
    "docs-version": "cross-env BUILD_DOC_VERSION=version npm run docs",
    "build": "father build",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:fix": "npm run lint-fix:js && npm run lint-fix:style",
    "lint:js": "eslint src --ext .ts,.tsx",
    "lint-fix:js": "npm run lint:js -- --fix",
    "lint:style": "stylelint src/**/*.less",
    "lint-fix:style": "npm run lint:style -- --fix",
    "prettier": "prettier --write **/*",
    "prepublishOnly": "npm run build",
    "commit": "cz",
    "tsc": "tsc --noEmit",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/doly-dev/antd-more.git"
  },
  "keywords": [
    "react",
    "react-components",
    "ant-design",
    "antd",
    "antd-more"
  ],
  "files": [
    "dist"
  ],
  "author": "caijf",
  "license": "MIT",
  "peerDependencies": {
    "@ant-design/icons": ">= 4.0.0",
    "antd": "^5.2.0",
    "react": ">= 16.9.0",
    "react-dom": ">= 16.9.0"
  },
  "devDependencies": {
    "@ant-design/icons": "^5.2.6",
    "@ant-design/pro-components": "^2.6.28",
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/mockjs": "^1.0.8",
    "@types/react": "^18.2.27",
    "@types/react-color": "^3.0.7",
    "@types/react-dom": "^18.2.12",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "antd": "^5.9.4",
    "commitlint": "^17.7.2",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dumi": "^2.2.12",
    "element-remove": "^1.0.4",
    "eslint": "^8.51.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "father": "^4.3.5",
    "husky": "^8.0.3",
    "lcn": "^5.0.6",
    "lint-staged": "^13.3.0",
    "mockjs": "^1.1.0",
    "prettier": "^2.8.8",
    "prettier-plugin-two-style-order": "^1.0.1",
    "qs": "^6.11.2",
    "rc-slider-captcha": "^1.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stylelint": "^14.16.1",
    "stylelint-config-css-modules": "^4.3.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.7.0",
    "stylelint-no-unsupported-browser-features": "^6.1.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.23.1",
    "cache2": "^1.1.1",
    "classnames": "^2.3.2",
    "countdown-pro": "^2.0.1",
    "dayjs": "^1.11.10",
    "rc-hooks": "^3.0.6",
    "rc-util": "^5.37.0",
    "react-color": "^2.19.3",
    "ut2": "^1.5.5",
    "util-helpers": "^4.20.8"
  },
  "lint-staged": {
    "**/*.less": "stylelint --fix",
    "**/*.{ts,tsx,js,jsx}": "eslint",
    "**/*.{ts,tsx,js,jsx,less,md}": "prettier -w"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
