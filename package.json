{
  "name": "antd-more",
  "version": "2.4.0",
  "description": "基于 antd 扩展的业务场景组件。",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": [
    "*.less"
  ],
  "scripts": {
    "start": "dumi dev",
    "doc": "dumi build",
    "doc-version": "cross-env BUIDL_DOC_VERSION=version npm run doc",
    "build": "father-build",
    "lint": "eslint src/* --ext .tsx,.ts",
    "prettier": "prettier --write 'src/**/*.tsx' && prettier --write 'src/**/*.ts'",
    "lint-staged": "lint-staged",
    "lint-staged:prettier": "prettier --write",
    "lint-staged:lint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-pattern 'src/*/demos/**'",
    "precommit": "lint-staged",
    "commit": "cz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/doly-dev/antd-more.git"
  },
  "keywords": [
    "react",
    "react-components",
    "ant-design",
    "antd",
    "antd-more"
  ],
  "files": [
    "es",
    "lib"
  ],
  "author": "caijf",
  "license": "MIT",
  "peerDependencies": {
    "@ant-design/icons": "^4.0.0",
    "antd": "^4.0.0",
    "react": ">= 16.9.0",
    "react-dom": ">= 16.9.0"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.3.0",
    "@ant-design/pro-layout": "^6.14.0",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/classnames": "^2.2.11",
    "@types/react": "^17.0.3",
    "@types/react-color": "^3.0.4",
    "@types/react-dom": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "antd": "^4.15.1",
    "babel-plugin-import": "^1.13.3",
    "commitlint": "^11.0.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dumi": "^1.1.4",
    "dumi-theme-default": "^1.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-doly-react": "0.0.3-alpha.0",
    "father-build": "^1.18.5",
    "husky": "^4.3.0",
    "invariant": "^2.2.4",
    "lcn": "3",
    "lint-staged": "^10.5.1",
    "mockjs": "^1.1.0",
    "prettier": "^2.2.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.0",
    "classnames": "^2.2.6",
    "countdown-pro": "^1.0.2",
    "moment": "^2.29.1",
    "rc-hooks": "^1.0.0",
    "react-color": "^2.19.3",
    "util-helpers": "^3.10.4"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": "npm run lint-staged:lint",
    "src/**/*.{ts,tsx,js,jsx,less}": "npm run lint-staged:prettier"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}