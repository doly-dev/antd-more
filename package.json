{
  "name": "antd-more",
  "version": "4.1.27",
  "description": "基于 antd 扩展的业务场景组件。",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "sideEffects": [
    "*.less"
  ],
  "scripts": {
    "start": "dumi dev",
    "docs": "dumi build",
    "docs-version": "cross-env BUILD_DOC_VERSION=version npm run docs",
    "build": "father build",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:fix": "npm run lint-fix:js && npm run lint-fix:style",
    "lint:js": "eslint src --ext .ts,.tsx",
    "lint-fix:js": "npm run lint:js -- --fix",
    "lint:style": "stylelint src/**/*.less",
    "lint-fix:style": "npm run lint:style -- --fix",
    "prettier": "prettier --write **/*",
    "lint-staged": "lint-staged",
    "precommit": "lint-staged",
    "prepublishOnly": "npm run build",
    "commit": "cz",
    "tsc": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/doly-dev/antd-more.git"
  },
  "keywords": [
    "react",
    "react-components",
    "ant-design",
    "antd",
    "antd-more"
  ],
  "files": [
    "dist"
  ],
  "author": "caijf",
  "license": "MIT",
  "peerDependencies": {
    "@ant-design/icons": "^4.8.0",
    "antd": "^5.0.3",
    "react": ">= 16.9.0",
    "react-dom": ">= 16.9.0"
  },
  "devDependencies": {
    "@ant-design/icons": "^5.0.1",
    "@ant-design/pro-components": "^2.4.4",
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@types/mockjs": "^1.0.7",
    "@types/react": "^18",
    "@types/react-color": "^3.0.6",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "antd": "^5.5.1",
    "commitlint": "^17.5.1",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dumi": "^2.1.19",
    "element-remove": "^1.0.4",
    "eslint": "^8.38.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "father": "^4.1.7",
    "husky": "^4.3.0",
    "lcn": "^5.0.3",
    "lint-staged": "^13.2.1",
    "mockjs": "^1.1.0",
    "prettier": "^2.8.7",
    "prettier-plugin-two-style-order": "^1.0.1",
    "qs": "^6.11.1",
    "rc-slider-captcha": "^1.1.8",
    "react": "^18",
    "react-dom": "^18",
    "stylelint": "^14.16.1",
    "stylelint-config-css-modules": "^4.2.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.7.0",
    "stylelint-no-unsupported-browser-features": "^6.1.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@babel/runtime": "^7.21.0",
    "cache2": "^1.0.2",
    "classnames": "^2.3.2",
    "countdown-pro": "^2.0.1",
    "dayjs": "^1.11.7",
    "rc-hooks": "^2.3.2",
    "rc-util": "^5.29.3",
    "react-color": "^2.19.3",
    "util-helpers": "^4.17.3"
  },
  "lint-staged": {
    "**/*.less": "stylelint --fix",
    "**/*.{ts,tsx,js,jsx}": "eslint",
    "**/*.{ts,tsx,js,jsx,less,md}": "prettier -w"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
